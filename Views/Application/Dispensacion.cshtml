@model Tarea1_Leonardo.services.ServicesBilletes

@{
    ViewData["Title"] = "Dispensacion";
}

<div class="card-header bg-dark text-light" style="margin-top: 40px">
    <h5 class="text-center"><strong>Recibo</strong></h5>
</div>
<table class="table caption-top">
    <thead>
        <tr>
            <th scope="col">Modo de Dispensacion</th>
            <th scope="col">Monto a retirar</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@ViewBag.Retiro</td>
            <td>@ViewBag.Dispensacion</td>
        </tr>
    </tbody>
</table>

<div class="card-header bg-dark text-light" style="margin-top: 40px">
    <h5 class="text-center"><strong>Factura de Dispensacion</strong></h5>
</div>
<table class="table caption-top">
    <thead>
        <tr>
            <th scope="col">Papeletas</th>
            <th scope="col">Cantidad</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">100</th>
            <td>@ViewBag.CantidadUno</td>
        </tr>
        <tr>
            <th scope="row">200</th>
            <td>@ViewBag.CantidadDos</td>
        </tr>
        <tr>
            <th scope="row">500</th>
            <td>@ViewBag.CantidadTres</td>
        </tr>
        <tr>
            <th scope="row">1000</th>
            <td>@ViewBag.CantidadCuatro</td>
        </tr>
    </tbody>
</table>